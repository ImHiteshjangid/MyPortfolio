{
    "name": "personal-website",
    "author": "Hitesh Jangid",
    "homepage": " ",
    "version": "2.0.0",
    "private": true,
    "dependencies": {
        "@fortawesome/fontawesome-free": "^6.6.0",
        "@fortawesome/fontawesome-svg-core": "^6.6.0",
        "@fortawesome/free-solid-svg-icons": "^6.6.0",
        "@fortawesome/react-fontawesome": "^0.2.2",
        "bootstrap": "^5.3.3",
        "gh-pages": "^6.1.1",
        "react": "^18.2.0",
        "react-bootstrap": "^2.10.2",
        "react-dom": "^18.2.0",
        "react-icons": "^5.2.1",
        "react-scripts": "5.0.1",
        "react-tsparticles": "^2.12.2",
        "react-vertical-timeline-component": "^3.6.0",
        "yarn": "^1.22.22"
    },
    "scripts": {
        "predeploy": "yarn run build",
        "deploy": "gh-pages -d build",
        "direct-deploy": "gh-pages -d build",
        "clean": "rimraf node_modules dist build && del *.log",
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "sitemap": "node ./src/sitemap/sitemap-generator.js",
        "purge": "purgecss --css ./build/static/css/*.css --content ./build/index.html build/static/js/*.js --output ./dist/"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "eslint-config-react-app": "^7.0.1",
        "rimraf": "^6.0.1"
    }
